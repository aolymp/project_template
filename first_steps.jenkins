#!groovy
// Check puppet-rh6  hostname
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                sh 'ssh jenkins@puppet-rh6 \'hostname\''
            }
        }
    }
}
